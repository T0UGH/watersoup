.open_gid {
  width: 400rpx;
  overflow: hidden;
}
.instruction_view{
  position:fixed;
  display: flex;
  bottom: 40rpx;
}
.instruction{
  font-size: 28rpx;
  color: #707070;
}
.logo{
  width: 200rpx;
  height: 200rpx;
  margin-left:235rpx;
  margin-top: 110rpx;
}
.content{
  margin-top: 40rpx;
  display: flex;
  text-align: center;
  line-height: 100rpx;
   font-weight: bold;  
}
.identity{
  width: 100rpx;
}
page{
  background-color: #f5f5f5;
  box-shadow: 0rpx 5rpx 5rpx #888888 inset;
}
.header{
  display: flex;
  flex-direction:row;
  margin-bottom: 150rpx;
  height:300rpx;
  border-radius: 75%;
  margin-top:-150rpx;
  background-color: #01B703;
}
.img{
  width: 120rpx; 
  height: 120rpx;
  border-radius: 50%;
  margin-top:240rpx;
  margin-left:315rpx;
  vertical-align: middle;
}
.share_button {
  background-color: rgb(68,138,255);
  color: #ffffff;
  font-size: 40rpx;
  margin-top: 120rpx;
  border-radius: 0rpx;  
  height: 80rpx;
  line-height: 80rpx;
  box-shadow: 0rpx 3rpx 3rpx #888888 ;
  margin-bottom: 40rpx;
}
.picker{
  display: inline;
}
.select{
  display: inline-block;
  overflow: hidden;
  width: 400rpx;
  text-align: left;
  margin-right: 120rpx;
}
.in_button{
  display:inline-block;
  background-color: rgb(68,138,255);
  color: #fff;
  font-size: 40rpx;
  border-radius: 0rpx;  
  height: 80rpx;
  line-height: 80rpx;
  box-shadow: 0rpx 3rpx 3rpx #888888 ;
}
.group_list{
  height: 280rpx;
  white-space:nowrap;
  background-color: white;
}